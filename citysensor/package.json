{
  "name": "bham_city_sensor",
  "version": "0.9.0",
  "description": "birmingham city sensor project",
  "main": "src/server.js",
  "scripts": {
    "lint": "eslint src --ext=js",
    "dev": "nodemon src/server.js",
    "build": "NODE_ENV=production next build src",
    "start": "NODE_ENV=production node src/server.js",
    "predist": "rm -rf dist && npm run build",
    "dist": "webpack -p --config src/webpack.server.config.js && npm run dist-assets",
    "dist-assets": "cp -R ./{certs,package.json,package-lock.json} dist && cp -R src/.next dist/src/.next && cp -R src/static dist/src/static",
    "postdist": "cd dist && npm i --production && tar -czvf dist-$npm_package_version.tar.gz -C ./ ."
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/codeforbirmingham/smarterbham"
  },
  "author": "Josh Flayhart, Fred Dorothy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codeforbirmingham/smarterbham/issues"
  },
  "dependencies": {
    "aws-iot-device-sdk": "2.2.0",
    "axios": "0.17.1",
    "body-parser": "1.18.2",
    "express": "4.16.2",
    "jss": "9.5.1",
    "lodash": "4.17.4",
    "material-ui": "1.0.0-beta.29",
    "next": "4.2.3",
    "nprogress": "0.2.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-jss": "8.2.1",
    "socket.io": "2.0.4",
    "socket.io-client": "2.0.4"
  },
  "devDependencies": {
    "babel-preset-env": "1.6.1",
    "eslint": "4.5.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "nodemon": "1.14.11",
    "webpack": "3.10.0",
    "webpack-node-externals": "1.6.0"
  }
}
